@{
    ViewBag.Title = "Contact Page";
}
<h2>Contact Me</h2>
<form>
    <div>
        <label>Name</label>
        <input/>
    </div>
    <div>
        <label>Email</label>
        <input type="email"/>
    </div>
    <div>
        <label>Message</label>
        <textarea cols="40" rows="5"></textarea>
    </div>
    <div>
        <input type="submit" value="Send Message"/>
    </div>
</form>
<script type="text/javascript" src="~/js/site.js"></script>